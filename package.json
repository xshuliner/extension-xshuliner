{
  "name": "extension-xshuliner",
  "private": true,
  "version": "0.0.1",
  "description": "一款高效优雅的日常工作助手，全面提升你的工作效率与体验。",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "WEBENV=uat PLATFORM=chrome vite",
    "dev:uat": "WEBENV=uat PLATFORM=chrome vite",
    "dev:prod": "WEBENV=prod PLATFORM=chrome vite",
    "build": "tsc -b && WEBENV=prod PLATFORM=chrome vite build",
    "build:uat": "tsc -b && WEBENV=uat PLATFORM=chrome vite build",
    "build:prod": "tsc -b && WEBENV=prod PLATFORM=chrome vite build",
    "preview": "vite preview",
    "lint": "tsc --noEmit && pnpm lint:eslint && pnpm lint:prettier && pnpm lint:cspell",
    "lint:eslint": "eslint",
    "lint:prettier": "prettier --write \"**/*.{js,ts,mjs,cjs,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:cspell": "cspell lint \"**/*.{js,ts,mjs,cjs,json,tsx,css,less,scss,vue,html,md}\"",
    "prepare": "husky",
    "release": "standard-version",
    "commit": "git-cz"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,json,tsx,css,less,scss,vue,html,md}": [
      "eslint",
      "prettier --write",
      "cspell lint"
    ]
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "axios": "^1.13.2",
    "blueimp-md5": "^2.19.0",
    "class-variance-authority": "^0.7.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.554.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/cz-commitlint": "^20.1.0",
    "@crxjs/vite-plugin": "^2.2.1",
    "@cspell/dict-lorem-ipsum": "^4.0.5",
    "@eslint/js": "^9.36.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@types/blueimp-md5": "^2.18.2",
    "@types/chrome": "^0.1.27",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/webextension-polyfill": "^0.12.4",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.22",
    "commitizen": "^4.3.1",
    "cspell": "^9.3.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "inquirer": "^9.3.8",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react-router-dom": "^7.9.5",
    "standard-version": "^9.5.0",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^6.0.11",
    "webextension-polyfill": "^0.12.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
