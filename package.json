{
  "name": "extension-xshuliner",
  "private": true,
  "version": "999.999.999",
  "description": "一款高效优雅的日常工作助手，全面提升你的工作效率与体验。",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:cspell",
    "lint:eslint": "eslint",
    "lint:prettier": "prettier --write \"**/*.{js,ts,mjs,cjs,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:cspell": "cspell lint \"**/*.{js,ts,mjs,cjs,json,tsx,css,less,scss,vue,html,md}\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,json,tsx,css,less,scss,vue,html,md}": [
      "eslint",
      "prettier --write",
      "cspell lint"
    ]
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.2.1",
    "@cspell/dict-lorem-ipsum": "^4.0.5",
    "@eslint/js": "^9.36.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@tailwindcss/vite": "^4.1.17",
    "@types/chrome": "^0.1.27",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/webextension-polyfill": "^0.12.4",
    "@vitejs/plugin-react": "^5.0.4",
    "classnames": "^2.5.1",
    "cspell": "^9.3.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react-router-dom": "^7.9.5",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^6.0.11",
    "webextension-polyfill": "^0.12.0"
  }
}
